<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="50.0" prefHeight="500.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TourProject.Controller">
    <rowConstraints>
        <RowConstraints prefHeight="35" />
        <RowConstraints prefHeight="35" />
        <RowConstraints prefHeight="35" />
        <RowConstraints prefHeight="35" vgrow="ALWAYS" />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints percentWidth="25" />
        <ColumnConstraints percentWidth="25" />
        <ColumnConstraints percentWidth="25" />
        <ColumnConstraints percentWidth="25" />
    </columnConstraints>
    <children>
        <Label style="-fx-padding: 5" text="Tour-Planner" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.rowIndex="0">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <MenuBar GridPane.columnSpan="4" GridPane.rowIndex="1">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Options">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <GridPane alignment="CENTER" maxWidth="850.0" GridPane.columnSpan="4" GridPane.rowIndex="2">
            <rowConstraints>
                <RowConstraints />
            </rowConstraints>
            <columnConstraints>
                <ColumnConstraints percentWidth="75" />
                <ColumnConstraints percentWidth="25" />
            </columnConstraints>
            <TextField fx:id="inputSearch" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Search">
                <GridPane.margin>
                    <Insets right="5.0" />
                </GridPane.margin>
            </TextField>
            <Button defaultButton="true" maxWidth="250" onAction="#search" GridPane.columnIndex="1">
                <graphic>
                    <ImageView fitHeight="25.0" pickOnBounds="true" preserveRatio="true" GridPane.vgrow="NEVER">
                        <Image url="https://freeiconshop.com/wp-content/uploads/edd/search-outline-filled.png" />
                    </ImageView>
                </graphic>
                Search
                <GridPane.margin>
                    <Insets left="5.0" />
                </GridPane.margin>
            </Button>
            <GridPane.margin>
                <Insets />
            </GridPane.margin>
        </GridPane>


        <VBox maxHeight="1.7976931348623157E308" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="3">
            <children>
                <GridPane minHeight="20.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Button maxWidth="50.0" mnemonicParsing="false" text="+" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" />
                        <Button maxWidth="50.0" mnemonicParsing="false" text="-" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </GridPane>

                <TableView fx:id="toursListing" maxHeight="1.7976931348623157E308" VBox.vgrow="SOMETIMES">
                    <columns>
                        <TableColumn fx:id="tournameColumn" prefWidth="120.0" text="Tours" />
                    </columns>
                </TableView>
            </children>
            <padding>
                <Insets top="10.0" />
            </padding>
        </VBox>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="300.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="3">
            <children>
                <Label fx:id="tourLabel" text="Title: ">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </Label>
                <ButtonBar prefHeight="40.0" prefWidth="200.0" />
                <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="SOMETIMES">
                    <tabs>
                        <Tab text="Route">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                            </content>
                        </Tab>
                        <Tab text="Description">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="139.0" prefWidth="385.0">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <Label maxWidth="450.0" text="Label" wrapText="true" fx:id="tourDescription" />
                                 </children>
                              </GridPane>
                           </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
                <TableView fx:id="tourLogs" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="300.0" VBox.vgrow="SOMETIMES">
                    <columns>
                        <TableColumn fx:id="tourDate" prefWidth="75.0" text="Date" />
                        <TableColumn fx:id="tourDuration" prefWidth="75.0" text="Duration" />
                        <TableColumn fx:id="tourDistance" prefWidth="75.0" text="Distance" />
                        <TableColumn fx:id="tourAvgSpeed" prefWidth="75.0" text="Average SPD" />
                        <TableColumn prefWidth="75.0" text="..." />
                    </columns>
                </TableView>
            </children>
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </GridPane.margin>
        </VBox>

    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>

</GridPane>
